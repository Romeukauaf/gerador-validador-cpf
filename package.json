{
  "name": "gerador-validador-cpf",
  "version": "4.0.1",
  "description": "Biblioteca JS open-source para gerar, validar e formatar CPF.",
  "homepage": "http://tiagoporto.github.io/gerador-validador-cpf",
  "keywords": [
    "cpf",
    "gerador",
    "generator",
    "generates",
    "validador",
    "valida",
    "validates",
    "formata",
    "format"
  ],
  "author": {
    "name": "Tiago Porto",
    "url": "http://tiagoporto.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tiagoporto/gerador-validador-cpf.git"
  },
  "main": "./dist/CPF.umd.js",
  "module": "./dist/CPF.es.js",
  "typings": "./dist/CPF.d.ts",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --hot --config webpack.config.dev.js",
    "lint": "eslint --ext .ts,.js ./src --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "checks": "npm run lint && npm run check-typing",
    "check-typing": "tsc --noEmit",
    "check-formatting": "prettier --check './src/**/*.{js,jsx,ts,tsx,html}'",
    "formatting-write": "prettier --write './src/**/*.{js,jsx,ts,tsx,html}'",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:mutation": "stryker run --reporters html",
    "test:mutation-report": "stryker run --reporters dashboard",
    "test:coverage": "npm test -- --coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "types": "tsc ./src/lib/CPF.ts --declarationDir dist --emitDeclarationOnly --declaration",
    "build:site": "rm -rf site && webpack --mode=production",
    "deploy": "npm run build:site && gh-pages -d site",
    "prepublishOnly": "rm -rf dist && rollup -c && npm run types",
    "postpublish": "npm run tag && git push --tags",
    "tag": "git tag v$npm_package_version"
  },
  "dependencies": {
    "core-js": "^3.6.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@stryker-mutator/core": "^3.1.0",
    "@stryker-mutator/html-reporter": "^3.1.0",
    "@stryker-mutator/jest-runner": "^3.1.0",
    "@stryker-mutator/typescript": "^3.1.0",
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.34",
    "@types/react-copy-to-clipboard": "^4.3.0",
    "@types/react-dom": "^16.9.7",
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "babel-loader": "^8.1.0",
    "coveralls": "^3.1.0",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^6.0.0",
    "gh-pages": "^2.2.0",
    "html-webpack-plugin": "^4.2.0",
    "husky": "^4.2.5",
    "jest": "^25.4.0",
    "normalize.css": "^8.0.1",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.13.1",
    "react-github-corner": "^2.3.0",
    "react-imask": "^6.0.3",
    "rollup": "^2.7.2",
    "rollup-plugin-license": "^2.0.0",
    "rollup-plugin-terser": "^5.3.0",
    "style-loader": "^1.2.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "typescript": "^3.9.0-beta",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run checks"
    }
  }
}
